extends layout

block content
  h1= title
  p I am missing my sister.
  button(type="button" class="btn btn-secondary" onclick="boost()") Sample Button
  div(id="boost") Add in extra
  script.
    var value = 0;
    let amt = 3;

    function boost() {
      amt += 1;
      value += amt;
      document.getElementById("boost").innerText = `Boosted ${value} by ${amt}.`;
    }
  a(href="/artifacts") View Artifacts
